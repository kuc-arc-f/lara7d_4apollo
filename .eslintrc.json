{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],  
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },   
  "rules": {
    "no-var": "error",
    "no-console": "off",
    "no-extra-semi": "warn",
    "no-undef": "warn",
    "react/prop-types": "off"
  }  
}
